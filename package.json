{
  "name": "carts-api",
  "version": "0.1.0",
  "bin": {
    "carts-api": "bin/cartsapi.js"
  },
  "scripts": {
    "build": "npm run lint && tsc",
    "watch": "tsc -w",
    "test": "jest --forceExit",
    "test-coverage": "jest --coverage --forceExit",
    "test-view": "http-server -c-1 ./coverage/lcov-report",
    "deploy": "cdk deploy",
    "start": "npm run lint --force && nodemon",
    "lint": "eslint \"src/**/*.ts\" --quiet",
    "prettier": "prettier --write \"src/**/*.ts\"",
    "lint:fix": "eslint \"src/**/*.ts\" --quiet --fix && npm run prettier",
    "prepare": "husky install"
  },
  "dependencies": {
    "@aws-cdk/aws-cloudformation": "^1.198.0",
    "@types/aws-lambda": "^8.10.114",
    "@types/express": "^4.17.11",
    "@types/git-branch": "^2.0.2",
    "@types/jest": "^29.4.0",
    "@types/mongoose": "^5.10.3",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "@typescript-eslint/parser": "^5.61.0",
    "aws-cdk": "^2.81.0",
    "aws-cdk-lib": "^2.81.0",
    "aws-serverless-express": "^3.4.0",
    "axios": "^1.3.4",
    "constructs": "^10.1.293",
    "decimal.js": "^10.4.3",
    "dotenv": "^16.0.3",
    "esbuild": "^0.17.12",
    "eslint": "^8.41.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsdoc": "^44.2.4",
    "eslint-plugin-prettier": "^4.2.1",
    "express": "^4.17.1",
    "git-branch": "^2.0.1",
    "jest": "^29.5.0",
    "jsonwebtoken": "^9.0.0",
    "moment": "^2.29.4",
    "mongoose": "^5.13.16",
    "nodemon": "^2.0.7",
    "prettier": "^2.8.7",
    "saslprep": "^1.0.3",
    "serverless-http": "^2.7.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "~4.9.5"
  },
  "devDependencies": {
    "@types/dotenv": "^8.2.0",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/node": "^20.1.1",
    "http-server": "^14.1.1",
    "husky": "^8.0.3"
  }
}
